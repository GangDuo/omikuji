{
  "compilerOptions": {
    "typeRoots": [
      "types",
      "node_modules/@types"
    ],
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "lib": [
      "es2018",
      "dom"
    ],
    "strict": true,
    "resolveJsonModule": true,
  }
}
